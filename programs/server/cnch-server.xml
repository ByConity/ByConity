<?xml version="1.0" ?>
<yandex>
    <logger>
        <level>debug</level>
        <log>{your_path}/log/clickhouse-server.log</log>
        <errorlog>{your_path}/log/clickhouse-server.err.log</errorlog>
        <testlog>{your_path}/log/clickhouse-server.test.log</testlog>
        <size>1000M</size>
        <count>10</count>
    </logger>

    <http_port>42339</http_port>
    <rpc_port>23751</rpc_port>
    <tcp_port>59621</tcp_port>
    <ha_tcp_port>63357</ha_tcp_port>
    <exchange_port>37227</exchange_port>
    <exchange_status_port>51523</exchange_status_port>

    <openSSL>
        <server>

            <certificateFile>/etc/clickhouse-server/server.crt</certificateFile>
            <privateKeyFile>/etc/clickhouse-server/server.key</privateKeyFile>

            <dhParamsFile>/etc/clickhouse-server/dhparam.pem</dhParamsFile>
            <verificationMode>none</verificationMode>
            <loadDefaultCAFile>true</loadDefaultCAFile>
            <cacheSessions>true</cacheSessions>
            <disableProtocols>sslv2,sslv3</disableProtocols>
            <preferServerCiphers>true</preferServerCiphers>
        </server>

        <client>
            <loadDefaultCAFile>true</loadDefaultCAFile>
            <cacheSessions>true</cacheSessions>
            <disableProtocols>sslv2,sslv3</disableProtocols>
            <preferServerCiphers>true</preferServerCiphers>

            <invalidCertificateHandler>

                <name>AcceptCertificateHandler</name>

            </invalidCertificateHandler>
        </client>
    </openSSL>

    <interserver_http_port>30977</interserver_http_port>

    <listen_host>0.0.0.0</listen_host>

    <max_connections>4096</max_connections>
    <keep_alive_timeout>3</keep_alive_timeout>

    <max_concurrent_queries>100</max_concurrent_queries>

    <uncompressed_cache_size>8589934592</uncompressed_cache_size>

    <mark_cache_size>5368709120</mark_cache_size>

    <path>{your_path}</path>

    <tmp_path>{your_path}/tmp_data/</tmp_path>

    <users_config>{your_path}/cnch-users.xml</users_config>

    <default_profile>default</default_profile>

    <default_database>default</default_database>

    <timezone>Europe/Moscow</timezone>

    <mlock_executable>false</mlock_executable>

    <remote_servers incl="clickhouse_remote_servers"></remote_servers>

    <zookeeper>
        <node index="1">
            <host>10.20.204.92</host>
            <port>2181</port>
        </node>
    </zookeeper>

    <macros incl="macros" optional="true"/>

    <builtin_dictionaries_reload_interval>3600</builtin_dictionaries_reload_interval>

    <max_session_timeout>3600</max_session_timeout>

    <default_session_timeout>60</default_session_timeout>

    <dictionaries_config>*_dictionary.xml</dictionaries_config>

    <compression incl="clickhouse_compression">
    </compression>

    <distributed_ddl>

        <path>/clickhouse/task_queue/ddl</path>

    </distributed_ddl>

    <graphite_rollup_example>
        <pattern>
            <regexp>click_cost</regexp>
            <function>any</function>
            <retention>
                <age>0</age>
                <precision>3600</precision>
            </retention>
            <retention>
                <age>86400</age>
                <precision>60</precision>
            </retention>
        </pattern>
        <default>
            <function>max</function>
            <retention>
                <age>0</age>
                <precision>60</precision>
            </retention>
            <retention>
                <age>3600</age>
                <precision>300</precision>
            </retention>
            <retention>
                <age>86400</age>
                <precision>3600</precision>
            </retention>
        </default>
    </graphite_rollup_example>

    <format_schema_path>{your_path}/format_schemas/</format_schema_path>

    <!-- Service discovery is similar to old config, except for TSO port (now TSO RPC port is PORT0) -->
    <service_discovery>
        <mode>local</mode>
        <cluster>default</cluster>
        <disable_cache>false</disable_cache>
        <cache_timeout>5</cache_timeout>
        <server>
            <psm>data.cnch.server</psm>

            <service>cnch-server</service>
            <headless_service>cnch-server-headless</headless_service>

            <node>
                <host>10.130.48.166</host>
                <hostname>Server-6</hostname>
                <ports>
                    <port>
                        <!--HTTP-->
                        <name>PORT2</name>
                        <value>42339</value>
                    </port>
                    <port>
                        <!--RPC-->
                        <name>PORT1</name>
                        <value>23751</value>
                    </port>
                    <port>
                        <!--TCP/Primary-->
                        <name>PORT0</name>
                        <value>59621</value>
                    </port>
                    <port>
                        <!--HTTPS_PORT-->
                        <name>PORT4</name>
                        <value>47569</value>
                    </port>
                    <port>
                        <!--TCP_SECURE-->
                        <name>PORT3</name>
                        <value>18919</value>
                    </port>
                    <port>
                        <!--Exchange Stream Port-->
                        <name>PORT5</name>
                        <value>37227</value>
                    </port>
                    <port>
                        <!--Exchange Status Port-->
                        <name>PORT6</name>
                        <value>51523</value>
                    </port>
                </ports>
            </node>
        </server>
        <tso>
            <psm>data.cnch.tso</psm>

            <service>cnch-tso</service>
            <headless_service>cnch-tso-headless</headless_service>

            <node>
                <host>10.130.48.166</host>
                <hostname>TSO-0</hostname>
                <ports>
                    <port>
                        <!--TCP/Primary-->
                        <name>PORT0</name>
                        <value>49963</value>
                    </port>
                </ports>
            </node>
        </tso>
        <daemon_manager>
            <psm>data.cnch.daemon_manager</psm>

            <service>cnch-daemonmanager</service>
            <headless_service>cnch-daemonmanager-headless</headless_service>

            <node>
                <host>10.130.48.166</host>
                <hostname>Daemon-1</hostname>
                <ports>
                    <port>
                        <!--RPC-->
                        <name>PORT0</name>
                        <value>10635</value>
                    </port>
                </ports>
            </node>
        </daemon_manager>
        <vw_psm>data.cnch.vw_default</vw_psm>
        <vw>
            <psm>data.cnch.vw_default</psm>

            <node>
                <host>10.130.48.166</host>
                <hostname>WriteWorker-3</hostname>
                <ports>
                    <port>
                        <!--HTTP-->
                        <name>PORT2</name>
                        <value>57255</value>
                    </port>
                    <port>
                        <!--RPC-->
                        <name>PORT1</name>
                        <value>33479</value>
                    </port>
                    <port>
                        <!--TCP/Primary-->
                        <name>PORT0</name>
                        <value>47595</value>
                    </port>
                    <port>
                        <!--HTTPS_PORT-->
                        <name>PORT4</name>
                        <value>20475</value>
                    </port>
                    <port>
                        <!--TCP_SECURE-->
                        <name>PORT3</name>
                        <value>10217</value>
                    </port>
                    <port>
                        <!--Exchange Stream Port-->
                        <name>PORT5</name>
                        <value>61499</value>
                    </port>
                    <port>
                        <!--Exchange Status Port-->
                        <name>PORT6</name>
                        <value>24835</value>
                    </port>
                </ports>
                <vw_name>vw_write</vw_name>
            </node>
            <node>
                <host>10.130.48.166</host>
                <hostname>DefaultWorker-5</hostname>
                <ports>
                    <port>
                        <!--HTTP-->
                        <name>PORT2</name>
                        <value>20705</value>
                    </port>
                    <port>
                        <!--RPC-->
                        <name>PORT1</name>
                        <value>35575</value>
                    </port>
                    <port>
                        <!--TCP/Primary-->
                        <name>PORT0</name>
                        <value>12205</value>
                    </port>
                    <port>
                        <!--HTTPS_PORT-->
                        <name>PORT4</name>
                        <value>60079</value>
                    </port>
                    <port>
                        <!--TCP_SECURE-->
                        <name>PORT3</name>
                        <value>53093</value>
                    </port>
                    <port>
                        <!--Exchange Stream Port-->
                        <name>PORT5</name>
                        <value>21471</value>
                    </port>
                    <port>
                        <!--Exchange Status Port-->
                        <name>PORT6</name>
                        <value>47681</value>
                    </port>
                </ports>
                <vw_name>vw_default</vw_name>
            </node>
        </vw>
    </service_discovery>

    <catalog>
        <name_space>{your_name_space}</name_space>
    </catalog>

    <cnch_type>server</cnch_type>
    <perQuery>1</perQuery>

    <catalog_service>
        <!--Metastore storage type, support `bytekv` and `fdb`-->
        <type>bytekv</type>
        <bytekv>
            <service_name>bytedance.bytekv.bytekv_default</service_name>
            <cluster_name>bytekv_default</cluster_name>
            <name_space>bytekv_cnch_dev</name_space>
            <table_name>cnch_metastore_hz_dev</table_name>
        </bytekv>
        <fdb>
            <cluster_file>/path/to/fdb/cluster_config</cluster_file>
        </fdb>
    </catalog_service>

    <daemon_manager>
        <port>10635</port>
        <daemon_jobs>
        </daemon_jobs>
    </daemon_manager>

    <tso_service>
        <port>49963</port>
        <!--Metastore storage type, support `bytekv` and `fdb`-->
        <type>bytekv</type>
        <bytekv>
            <service_name>bytedance.bytekv.bytekv_default</service_name>
            <cluster_name>bytekv_default</cluster_name>
            <name_space>bytekv_cnch_dev</name_space>
            <table_name>cnch_metastore_hz_dev</table_name>
            <key_name>{your_tso_name_space}</key_name>
        </bytekv>
        <fdb>
            <cluster_file>/path/to/fdb/cluster_config</cluster_file>
        </fdb>
        <bytejournal>
            <renewal_interval_ms>1000</renewal_interval_ms>
            <polling_interval_ms>1000</polling_interval_ms>
            <error_retry_interval_ms>1000</error_retry_interval_ms>
            <lease_interval_ms>3000</lease_interval_ms>
            <failover_interval_ms>4000</failover_interval_ms>
            <election_point_key>tso_election_point_canh_1_1</election_point_key>
        </bytejournal>
        <tso_window_ms>3000</tso_window_ms>

        <tso_get_leader_info_interval_ms>0</tso_get_leader_info_interval_ms>
        <tso_max_retry_count>3</tso_max_retry_count>
    </tso_service>

    <storage_configuration>
        <disks>
            <default></default>
            <server_local_disk1>
                <path>{your_local_path}</path>
            </server_local_disk1>
            <server_hdfs_disk0>
                <path>{your_hdfs_path}</path>
                <type>bytehdfs</type>
            </server_hdfs_disk0>
        </disks>
        <policies>
            <default>
                <volumes>
                    <local>
                        <default>default</default>
                        <disk>default</disk>
                        <disk>server_local_disk1</disk>
                    </local>
                </volumes>
            </default>
            <cnch_default_hdfs>
                <volumes>
                    <hdfs>
                        <default>server_hdfs_disk0</default>
                        <disk>server_hdfs_disk0</disk>
                    </hdfs>
                </volumes>
            </cnch_default_hdfs>
        </policies>
    </storage_configuration>

    <hdfs_nnproxy>data.bytehouse.nnproxy-hostnetwork.service.lf</hdfs_nnproxy>

    <cnch_kafka_log>
        <database>cnch_system</database>
        <table>cnch_kafka_log</table>
        <flush_max_row_count>1000</flush_max_row_count>
        <flush_interval_milliseconds>7500</flush_interval_milliseconds>
    </cnch_kafka_log>

    <part_allocation_algorithm>1</part_allocation_algorithm>
    <consistent_hash_ring>
        <num_replicas>16</num_replicas>
        <num_probes>21</num_probes>
        <load_factor>1.3</load_factor>
    </consistent_hash_ring>
</yandex>
