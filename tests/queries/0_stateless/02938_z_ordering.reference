CREATE TABLE test_02938.wide_table\n(\n    `x` UInt32,\n    `y` UInt32,\n    `z` UInt32,\n    INDEX autoZcreate_x x TYPE minmax GRANULARITY 2,\n    INDEX autoZcreate_y y TYPE minmax GRANULARITY 2,\n    INDEX autoZcreate_z z TYPE minmax GRANULARITY 2\n)\nENGINE = MergeTree\nPRIMARY KEY tuple()\nORDER BY mortonEncode(x, y, z)\nSETTINGS z_index_granularity = 2, index_granularity = 5
