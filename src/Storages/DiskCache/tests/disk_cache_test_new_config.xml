<?xml version="1.0"?>
<yandex>
    <disk_cache_strategies>
        <File>
            <hits_to_cache>1</hits_to_cache>
            <segment_size>1</segment_size>
            <latest_disk_cache_dir>file_disk_cache</latest_disk_cache_dir>
        </File>
        <MergeTree>
            <hits_to_cache>1</hits_to_cache>
            <segment_size>1</segment_size>
            <latest_disk_cache_dir>mergetree_disk_cache</latest_disk_cache_dir>
        </MergeTree>
        <Hive>
            <hits_to_cache>1</hits_to_cache>
            <segment_size>1</segment_size>
            <latest_disk_cache_dir>hive_disk_cache</latest_disk_cache_dir>
        </Hive>
    </disk_cache_strategies>
    <storage_configuration>
        <disks>
            <default></default>
            <server_local_disk1>
                <path>/tmp/</path>
            </server_local_disk1>
        </disks>
        <policies>
            <default>
                <volumes>
                    <local>
                        <default>default</default>
                        <disk>default</disk>
                        <disk>server_local_disk1</disk>
                    </local>
                </volumes>
            </default>
        </policies>
    </storage_configuration>
    <hdfs_nnproxy>nnproxy</hdfs_nnproxy>
</yandex>
